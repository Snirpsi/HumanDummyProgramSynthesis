#!/usr/bin/python3
if __name__ == '__main__':
    """ A function that removes all ports or stores numbers. """    
    ports = input("Enter ports separated by comma: ")
    ports = ports.split(",")
    ports = [int(port) for port in ports]
    
    ports_removed = [port for port in ports if port not in range(0,65535)]
    
    print("Ports removed: ", ports_removed)
