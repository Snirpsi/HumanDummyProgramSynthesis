if __name__ == '__main__': 
    
    words = ['hello', 'world', 'python', 'is', 'cool']
    
    words_to_hash = {}
    """ A hash map that iterates a list of words. """    
    
    for word in words:
        words_to_hash[word] = words_to_hash.get(word, 0) + 1
    
    for word, count in words_to_hash.items():
        print word, count
