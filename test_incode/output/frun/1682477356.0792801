if __name__ == '__main__': 
    """ A programm that removes a list of words. """    
    import sys
    
    if len(sys.argv) != 2:
        print "Usage: %s words.txt" % sys.argv[0]
        sys.exit()
    
    words = open(sys.argv[1]).read().split()
    
    removed = [w for w in words if w not in words]
    
    open(sys.argv[1], 'w').write('\n'.join(removed))
