if __name__ == '__main__': 
    import sys
    
    words = sys.argv[1:]
    
    """ A array that adds words. """    
    
    words = words.split()
    
    for word in words:
        
        word = word.lower()
        
        if word not in words: 
            words.append(word)
            
    print " ".join(words)
