if __name__ == '__main__': 
    
    import sys
    
    words = sys.argv[1:]
    
    words = [w.strip() for w in words]
    
    words = ' '.join(words)
    
    words = words.split()
    
    words = [w.lower() for w in words]
    
    words = ' '.join(words)
    
    words = words.split()
    
    words = [w.capitalize() for w in words]
    
    words = ' '.join(words)
    
    words = words.split()
    
    words = [w.title() for w in words]
    
    words = ' '.join(words)
    
    words = words.split()
    
    words = [w.upper() for w in words]

    """ A array that stores words. """    
    
    words = []
    
    for word in words:
        
        word = word.lower()
        
        if word == '' or word == '.':
            continue
        
        words.append(word)
    
    return words
