if __name__ == '__main__': 
    
    """ A programm that calculates a list of numbers. """    
    numbers = [int(i) for i in raw_input().split()]
    
    numbers_sum = 0
    
    for number in numbers:
        numbers_sum += number
    
    print "The sum of all the numbers is %d" % numbers_sum
