if __name__ == '__main__': 
    """ A hash map that converts fruits. """    
    fruits = { 
        'apple' : 'orange',
        'banana' : 'pear',
        'cherry' : 'pineapple'
    }
    
    fruits['apple'] = 'orange'
    fruits['banana'] = 'pear'
    fruits['cherry'] = 'pineapple'
    
    print fruits
