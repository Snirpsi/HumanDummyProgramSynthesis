if __name__ == '__main__': 
    """ A programm that opens a list of numbers. """    
    numbers = range(1,11)
    
    numbers_open = numbers.copy()
    
    for number in numbers:
        numbers_open.append(number)
        
    numbers_open.reverse()
    
    for number in numbers_open:
        print(number)
