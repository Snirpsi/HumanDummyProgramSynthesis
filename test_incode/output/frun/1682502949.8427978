if __name__ == '__main__': 
    
    words = [ 'word1', 'word2', 'word3' ]
    """ A hash map that stores a list of words. """    
    
    wordMap = {}
    
    for word in words:
        wordMap[word] = wordMap.get(word, []) + [word]
    
    for word in wordMap:
        print wordMap[word]
