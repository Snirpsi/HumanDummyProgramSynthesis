    numbers_sum = sum( numbers )
    
    numbers_average = sum( numbers) / ( len( numbers ) )
    
    numbers_median = sorted( numbers, key = lambda number: number % 2 )[-1]
    
    numbers_mode = sorted( numbers, key = lambda number: number % 2 )[0]
    
